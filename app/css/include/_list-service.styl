.list-service__link
	display inline-block
	margin-bottom 8px
	padding-bottom 8px
	padding-left 20px 
	border-bottom 1px solid grey
	width 100%
	position relative
	color black
	&:before
		content '>'
		position absolute
		left 0px
		color red
	&:last-child 
		border-bottom 0px
	.title
		padding-left 20px